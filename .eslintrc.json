{
   "extends":[
      "airbnb",
      "prettier",
      "prettier/react",
      "eslint-config-prettier"
   ],
   "plugins":[
      "prettier"
   ],
   "parser":"babel-eslint",
	  "env": {
	     "jest": true,
	     "es6": true
      },
   "rules":{
      "import/no-unresolved":"off",
      "react/jsx-filename-extension":[
         1,
         {
            "extensions":[
               ".js",
               ".jsx"
            ]
         }
      ],
	  "no-use-before-define": "off",
	  "prettier/prettier": "error",
	  "react/destructuring-assignment": "disabled"
   }
}
